Name=\"h-screen bg-gray-100 overflow-auto\">\n        <ErrorComponent />\n\n        <section className=\" w-1/4 h-full absolute flex flex-col-reverse pb-14  \">\n          {notifications.length > 0 &&\n            notifications.map((notification) => {\n              return (\n                <Notification\n                  notification={notification}\n                  key={notification.id}\n                />\n              );\n            })}\n        </section>\n\n        <div className=\" rounded-2xl w-72  ml-4  bg-white fixed h-full\">\n          <div className=\"relative top-2 mb-2 h-full  pt-8 rounded\">\n            <div className=\"text-5xl pb-5 font-semibold text-center\">MTOG</div>\n            <div className=\"flex flex-col gap-2 text-center\">\n              <div>\n                {/* <NavLink\n                  to={`${url}/home`}\n                  activeClassName={styles.activelink}\n                  className={styles.link}\n                >\n                  Dashboard\n                </NavLink> */}\n              </div>\n              <div>\n                <NavLink\n                  to={`${url}/transactions`}\n                  activeClassName={styles.activelink}\n                  className={styles.link}\n                >\n                  Transactions\n                </NavLink>\n              </div>\n\n              <div>\n                <NavLink\n                  to={`${url}/history`}\n                  activeClassName={styles.activelink}\n                  className={styles.link}\n                >\n                  History\n                </NavLink>\n              </div>\n              <div>\n                <NavLink\n                  to={`${url}/redeem`}\n                  activeClassName={styles.activelink}\n                  className={styles.link}\n                >\n                  Redeem\n                </NavLink>\n              </div>\n            </div>\n            <div className=\" absolute bottom-6  w-full  flex justify-center\">\n              <button\n                className=\"py-1 rounded-lg border-2 w-full mx-4  hover:bg-red-500 hover:text-white z-0\"\n                onClick={() => {\n                  auth.signout(() => {\n                    props.history.push(\"/\");\n                  });\n                }}\n              >\n                <BiLogOutCircle className=\"inline\" />\n                Sign out\n              </button>\n            </div>\n          </div>\n        </div>\n\n        <div className=\" ml-80  flex flex-col pr-5 relative h-full gap-10\">\n          <MerchantTopBar />\n          <div className=\" bottom-4 relative rounded-2xl border bg-white w-full h-full\">\n            <Switch>\n              {myroutes.map((route, index) => (\n                <ProtectedMerchantRoute\n                  key={index}\n                  path={route.path}\n                  exact={route.exact}\n                  component={route.main}\n                />\n              ))}\n              <Route path=\"*\">\n                <Redirect to=\"/merchant/home\" />\n              </Route>\n            </Switch>\n          </div>\n        </div>\n      </div>\n    </React.Fragment>\n  );\n};\n","/home/e1f/Documents/Project/project-frontend/src/dashboard/TopBar.js",[],"/home/e1f/Documents/Project/project-frontend/src/dashboard/Notification.js",[],"/home/e1f/Documents/Project/project-frontend/src/dashboard/ErrorComponent.js",[],"/home/e1f/Documents/Project/project-frontend/src/auth/Merchantsignup.js",[],"/home/e1f/Documents/Project/project-frontend/src/auth/OrganisationSignup.js",[],"/home/e1f/Documents/Project/project-frontend/src/features/products/productsSLice.js",[],"/home/e1f/Documents/Project/project-frontend/src/dashboard/merchants/Merchantslist.js",[],"/home/e1f/Documents/Project/project-frontend/src/dashboard/merchants/OrganisationMerchants.js",[],"/home/e1f/Documents/Project/project-frontend/src/features/orgmerchants/orgmerchantSLice.js",[],"/home/e1f/Documents/Pr